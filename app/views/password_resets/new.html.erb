<% provide(:title, 'Reset password') %>
<h1>Reset password</h1>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for :user, url: password_resets_store_path, method: :post do |f|%>
            <%= f.text_field :email %>
            <%= f.submit %>
        <% end %>
    </div>
</div>
